<jxb:bindings version="1.0"
               xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <jxb:bindings schemaLocation="Core_Integrity_Manifest_v1_0_1.xsd" node="/xs:schema">

    <jxb:schemaBindings>
      <jxb:package name="org.trustedcomputinggroup.xml.schema.core_integrity_v1_0_1_"/>
    </jxb:schemaBindings>
    
    <jxb:bindings node="//xs:complexType[@name='PlatformClassType']/xs:attribute[@name='Class']">
      <jxb:property name="platformClassTypeAttributeClass"/>
    </jxb:bindings>

  </jxb:bindings>

  <jxb:bindings schemaLocation="Integrity_Report_Manifest_v1_0.xsd" node="/xs:schema">

    <jxb:schemaBindings>
      <jxb:package name="org.trustedcomputinggroup.xml.schema.integrity_report_v1_0_"/>
    </jxb:schemaBindings>
    
  </jxb:bindings>

  <jxb:bindings schemaLocation="xmldsig-core-schema.xsd" node="/xs:schema">

    <jxb:schemaBindings>
      <jxb:package name="org.w3._2000._09.xmldsig_"/>
    </jxb:schemaBindings>
    
  </jxb:bindings>

  <jxb:bindings schemaLocation="Simple_Object_v1_0.xsd" node="/xs:schema">

    <jxb:schemaBindings>
      <jxb:package name="org.trustedcomputinggroup.xml.schema.simple_object_v1_0_"/>
    </jxb:schemaBindings>

    <jxb:globalBindings>
      <jxb:javaType name="java.lang.String" xmlType="xs:IDREF" parseMethod="javax.xml.bind.DatatypeConverter.parseString" printMethod="javax.xml.bind.DatatypeConverter.printString" />
    </jxb:globalBindings>
  </jxb:bindings>

</jxb:bindings>
